{
    "name": "lumite-studios/laravel-blade-tiptap",
    "description": "Integrate Tiptap for Laravel with Blade, Livewire, and Alpine.",
    "type": "package",
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.0|^9.0",
        "livewire/livewire": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "LumiteStudios\\LaravelBladeTiptap\\": "src"
        }
    },
	"extra": {
		"laravel": {
			"providers": [
				"LumiteStudios\\LaravelBladeTiptap\\LaravelBladeTiptapServiceProvider"
			]
		}
	},
    "minimum-stability": "dev",
    "prefer-stable": true
}
